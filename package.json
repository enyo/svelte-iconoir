{
	"name": "@indaco/svelte-iconoir",
	"version": "3.3.1",
	"type": "module",
	"packageManager": "pnpm@7.27.1",
	"author": "indaco <github@mircoveltri.me>",
	"description": "Iconoir SVG icons to Svelte and SvelteKit.",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"iconoir",
		"icons",
		"icons-set",
		"svg-icons"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/indaco/svelte-iconoir"
	},
	"bugs": {
		"url": "https://github.com/indaco/svelte-iconoir/issues"
	},
	"dependencies": {
		"svelte": "^3.56.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "1.8.3",
		"@sveltejs/package": "1.0.2",
		"@types/cli-progress": "^3.11.0",
		"@types/svg-parser": "^2.0.3",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"cli-progress": "^3.12.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"picocolors": "1.0.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"publint": "^0.1.10",
		"rimraf": "^4.4.0",
		"svelte-check": "^3.1.4",
		"svg-parser": "^2.0.4",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	},
	"scripts": {
		"build": "rimraf dist && svelte-kit sync && svelte-package && pnpm postbuild",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "tsc --outDir build/scripts && node build/scripts/cleanAll.js",
		"dev": "svelte-kit sync && vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"generate:icons": "svelte-kit sync && tsc --outDir build/scripts && pnpm clean && node build/scripts/buildIconsDataset.js",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"postbuild": "pnpm exec publint ./dist",
		"preview": "vite preview"
	},
	"types": "index.d.ts",
	"sideEffects": false,
	"svelte": "./index.js"
}
