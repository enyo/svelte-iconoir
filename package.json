{
	"name": "@indaco/svelte-iconoir",
	"version": "3.1.0",
	"type": "module",
	"author": "indaco <github@mircoveltri.me>",
	"description": "Iconoir SVG icons to SvelteKit.",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"iconoir",
		"icons",
		"icons-set",
		"svg-icons"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/indaco/svelte-iconoir"
	},
	"bugs": {
		"url": "https://github.com/indaco/svelte-iconoir/issues"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.86",
		"@sveltejs/kit": "1.0.0-next.531",
		"@sveltejs/package": "1.0.0-next.5",
		"@types/cli-progress": "^3.11.0",
		"@types/svg-parser": "^2.0.3",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"cli-progress": "^3.11.2",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"svg-parser": "^2.0.4",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.2"
	},
	"scripts": {
		"dev": "svelte-kit sync && vite dev",
		"build": "rimraf dist && svelte-kit sync && svelte-package",
		"preview": "vite preview",
		"generate:icons": "svelte-kit sync && tsc --outDir build/scripts && pnpm clean && node build/scripts/buildIconsDataset.js",
		"clean": "tsc --outDir build/scripts && node build/scripts/cleanAll.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"types": "index.d.ts",
	"sideEffects": false,
	"svelte": "./index.js"
}
