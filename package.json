{
	"name": "@indaco/svelte-iconoir",
	"version": "2.6.2",
	"type": "module",
	"author": "indaco <github@mircoveltri.me>",
	"description": "Iconoir SVG icons to SvelteKit.",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"iconoir",
		"icons",
		"icons-set",
		"svg-icons"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/indaco/svelte-iconoir"
	},
	"bugs": {
		"url": "https://github.com/indaco/svelte-iconoir/issues"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.80",
		"@sveltejs/kit": "1.0.0-next.503",
		"@sveltejs/package": "1.0.0-next.5",
		"@types/cli-progress": "^3.11.0",
		"@types/svg-parser": "^2.0.3",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"cli-progress": "^3.11.2",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.18",
		"svg-parser": "^2.0.4",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.3"
	},
	"scripts": {
		"dev": "svelte-kit sync && vite dev",
		"build": "svelte-kit sync && vite build",
		"package": "pnpm clean:dist && svelte-kit sync && svelte-package",
		"preview": "vite preview",
		"generate:icons": "svelte-kit sync && tsc && pnpm clean && node build/scripts/buildIconsDataset.js",
		"clean": "tsc && node build/scripts/cleanAll.js",
		"clean:dist": "rimraf dist",
		"clean:all": "npm run clean && pnpm clean:dist",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .prettierignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore ."
	},
	"types": "index.d.ts",
	"svelte": "index.js",
	"sideEffects": false,
	"publishConfig": {
		"directory": "dist"
	},
	"exports": {
		".": {
			"import": "./index.js",
			"types": "./types.d.ts"
		}
	}
}
